    "version": "0.2.0",
    "configurations": [
        {
            "name": "Launch Dalle",
            "type": "go",
            "request": "launch",
            "mode": "debug",
            "program": "${workspaceFolder}/dalle",
            "args": ["--local"]
        },
        {
            "name": "Update Dalle Commands",
            "type": "go",
            "request": "launch",
            "mode": "debug",
            "program": "${workspaceFolder}/dalle",
            "args": ["--local", "--update"]
        },
        {
            "name": "Destroy Dalle Commands",
            "type": "go",
            "request": "launch",
            "mode": "debug",
            "program": "${workspaceFolder}/dalle",
            "args": ["--local", "--destroy"]
        }
    ]
}